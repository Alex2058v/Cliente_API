RewriteEngine On
RewriteRule ^(aplicacion/listar)$ api.php?opc=listar [L,QSA]
RewriteRule ^api/listar$ api.php?opc=listar [L,QSA]
RewriteRule ^api/insertar$ api.php?opc=insertar [L,QSA]
RewriteRule ^api/editar$ api.php?opc=editar [L,QSA]
RewriteRule ^api/eliminar/([0-9a-zA-Z_-]*)$ api.php?opc=eliminar&user_id=$1 [L,QSA]
RewriteRule ^api/obtener/([0-9a-zA-Z_-]*)$ api.php?opc=obtener&user_id=$1 [L,QSA]
